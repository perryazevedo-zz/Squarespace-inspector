{
 "background": {"scripts": ["background.js"]},
 "browser_action": {
 "default_icon": "icon-128.png",
 "default_title": "Squarespace Inspector"
 },
 "name": "Squarespace Inspector",
 "description": "Chrome extension that checks to see if a website is built on Squarespace.",
 "homepage_url": "https://github.com/perryazevedo/Squarespace-inspector",
 "icons": {
     "16": "icon-16.png",
     "48": "icon-48.png",
     "128": "icon-128.png" },
 "permissions": [
     "tabs",
     "http://*/*",
     "https://*/*"
 ],
 "version": "0.2",
 "manifest_version": 2
}